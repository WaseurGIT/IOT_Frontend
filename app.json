{
  "expo": {
    "name": "iot_frontend",
    "slug": "iot_frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appicon.jpg",
    "scheme": "iotfrontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to save captured images to your photo library.",
        "NSPhotoLibraryUsageDescription": "This app needs access to save captured images to your photo library.",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to save captured images to your photo library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/appicon.jpg",
        "backgroundImage": "./assets/images/appicon.jpg",
        "monochromeImage": "./assets/images/appicon.jpg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.asifahammednishst.iot_frontend",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE"
      ],
      "usesCleartextTraffic": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/appicon.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/appicon.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow ESP32 Camera to save images to your photo library.",
          "savePhotosPermission": "Allow ESP32 Camera to save images to your photo library.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
